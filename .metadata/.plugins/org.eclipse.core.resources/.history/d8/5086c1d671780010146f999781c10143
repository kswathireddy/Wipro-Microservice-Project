package com.wipro.swathi.demo.feign;

import org.springframework.cloud.openfeign.FeignClient;
import org.springframework.data.domain.Page;
import org.springframework.data.domain.Pageable;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.PathVariable;

import com.wipro.swathi.demo.entity.PackageTour;

@FeignClient(name="Destination-Service",path="/api/v1/destination")
public interface PackageInterfae {
	

	@GetMapping("/getById/{id}")
	public PackageTour getById(@PathVariable Long id);
	@GetMapping("/allPackages")
	public Page<PackageTour> getAll(Pageable pageable);
	
	
}
