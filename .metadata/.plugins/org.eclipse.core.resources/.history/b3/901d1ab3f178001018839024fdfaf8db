import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
	@AllArgsConstructor
	@NoArgsConstructor
	public class BookingDTO {
	    private Long id;
	    private UserDTO user;
	    private PackageTourDTO packageInfo;
	    private LocalDate startDate;
	    private Status status; // confirmed, cancelled, pending
	}




